{
  "name": "timeflydev",
  "displayName": "TimeFly Dev",
  "description": "Track your coding time and activity",
  "version": "1.3.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "publisher": "TimeFly",
  "icon": "images/icon.png",
  "keywords": [
    "time tracking",
    "productivity",
    "coding"
  ],
  "galleryBanner": {
    "color": "#d47a5c",
    "theme": "dark"
  },
  "pricing": "Free",
  "homepage": "https://timefly.dev",
  "bugs": {
    "url": "https://timefly.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeFly-Dev/vs-code-extension.git",
    "private": true
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "contributes": {
    "commands": [
      {
        "command": "timefly.addApiKey",
        "title": "TimeFly: Add API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "1.96.0",
    "@types/mocha": "10.0.10",
    "@types/node": "22.15.23",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "cross-env": "^7.0.3",
    "eslint": "9.27.0",
    "esbuild": "0.25.5",
    "npm-run-all": "4.1.5",
    "typescript": "5.8.3",
    "@vscode/test-cli": "0.0.11",
    "@vscode/test-electron": "2.5.2"
  }
}