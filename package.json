{
  "name": "timeflydev",
  "displayName": "TimeFly Dev",
  "description": "Track your coding time and activity",
  "version": "1.3.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "publisher": "TimeFly",
  "icon": "images/icon.png",
  "keywords": [
    "time tracking",
    "productivity",
    "coding"
  ],
  "galleryBanner": {
    "color": "#d47a5c",
    "theme": "dark"
  },
  "pricing": "Free",
  "homepage": "https://timefly.dev",
  "bugs": {
    "url": "https://timefly.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeFly-Dev/vs-code-extension.git",
    "private": true
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "contributes": {
    "commands": [
      {
        "command": "timefly.addApiKey",
        "title": "TimeFly: Add API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "1.99.1",
    "@types/mocha": "10.0.10",
    "@types/node": "22.14.1",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "cross-env": "^7.0.3",
    "eslint": "9.24.0",
    "esbuild": "0.25.2",
    "npm-run-all": "4.1.5",
    "typescript": "5.8.3",
    "@vscode/test-cli": "0.0.10",
    "@vscode/test-electron": "2.5.2"
  }
}